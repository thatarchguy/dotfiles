! vim: set filetype=xdefaults :
*foreground: #D7D0C7
*background: #151515


xterm*faceName: Liberation Mono:size=8:antialias=true
!xterm*faceName: Liberation Mono:size=16:antialias=true
!xterm*font: 7x13

!xterm*faceName:Inconsolata:style=Regular:size=10


!black
! *color0:     #AF875F
! *color8:     #AF875F
*color0:     #101010
*color8:     #404040
!red
*color1:     #E84F4F
*color9:     #D23D3D
!green
*color2:     #B8D68C
*color10:    #A0CF5D
!yellow
*color3:     #E1AA5D
*color11:    #F39D21
!blue
*color4:     #7DC1CF
*color12:    #4E9FB1
!magenta
*color5:     #9B64FB
*color13:    #8542FF
!cyan
*color6:     #6D878D
*color14:    #42717B
!white
*color7:     #dddddd
*color15:    #dddddd


!URxvt*font:              xft:Literation Mono Nerd Font Complete Mono:size=8:antialias=true
URxvt*font:              xft:Liberation Mono:size=8:antialias=true
!URxvt*font:              xft:Inconsolata:size=10:antialias=true
!URxvt*font:			  xft:Andale Mono:size=8:antialias=true
!URxvt*font: xft:DejaVu Sans Mono:size=8
URxvt*letterSpace:        -1
Xft*antialias: true
Xft*hinting: true
!xft.dpi: 160

URxvt*scrollBar:          false
URxvt*scrollBar_right:    true
URxvt*scrollColor:        #000000
urxvt.saveLines:         10000

urxvt*transparent:        false
urxvt*shading:            30
URxvt*colorBD:            #ffffff
URxvt*cursorColor:        #ffffff
URxvt*cursorColor2:       #ffffff

urxvt*perl-lib:    /usr/lib/urxvt/perl/
URxvt.perl-ext-common: matcher,resize-font
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
URxvt.colorUL: #4682B4
